{
    "builds":[
        {
            "src":"afrismart_project/AfrimartCore/wsgi.py",
            "use":"@vercel/python",
            "config":{
                "maxLambdaSize":"200mb",
                "runtime":"python3.12"
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"afrismart_project/AfrimartCore/wsgi.py"
        }
    ]
}
